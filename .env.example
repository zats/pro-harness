# Copy keys into .env (do not commit .env)
OPENAI_API_KEY=

# Models (override as needed)
HARNESS_MODEL_THINKING=gpt-5.2
HARNESS_MODEL_CHEAP=gpt-5-mini

# Reasoning effort for thinking model: low|medium|high
HARNESS_REASONING_EFFORT=high

# Optional pricing map (USD per 1M tokens). If set, the harness prints total run cost.
# This repo ships with defaults for gpt-5.2 and gpt-5-mini; override here if you want.
# HARNESS_PRICING_USD_PER_1M_TOKENS_JSON={"gpt-5.2":{"input":1.75,"cached_input":0.175,"output":14},"gpt-5-mini":{"input":0.25,"cached_input":0.025,"output":2}}

# Web search tool call pricing (USD per 1K tool calls). Default: 10.
# HARNESS_WEB_SEARCH_USD_PER_1K_CALLS=10

# Runtime
HARNESS_MAX_STEPS=20
HARNESS_PRETTY=1
HARNESS_VERBOSITY=0
